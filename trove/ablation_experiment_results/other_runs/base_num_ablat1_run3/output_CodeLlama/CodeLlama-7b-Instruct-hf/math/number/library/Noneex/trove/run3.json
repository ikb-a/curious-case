# %%  (function)

def double_factorial(n):
    product = 1
    for i in range(1, n+1, 2):
        product *= i
    return product


# %%  (function)

def factorial(n):
    if n == 0:
        return 1
    else:
        return n * factorial(n-1)


# %%  (function)

def gcd(a, b):
    while b:
        a, b = b, a % b
    return a


# %%  (function)

def get_divisors(n):
    divisors = []
    for i in range(1, n + 1):
        if n % i == 0:
            divisors.append(i)
    return divisors


